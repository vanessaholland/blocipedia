<h1>Wikis</h1>
<div class="row">
   <div class="col-md-8">
     <% @wikis.each do |wiki| %>
      <%if (wiki.private && wiki.user_id == current_user.id) || !wiki.private || wiki.collaborations.include?(current_user)%>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to markdown(wiki.title), wiki %>
          </h4>
        </div>
      </div>
      <%end%>
     <% end %>
   </div>
   <div class="col-md-4">
     <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
   </div>
 </div>
